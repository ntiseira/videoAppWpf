<Window x:Class="VideoStudioApp.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStyle="None" ResizeMode="NoResize"  WindowState="Maximized" WindowStartupLocation="CenterScreen" 
        Loaded="Window_Loaded"
        Title="MainWindow" Height="768" Width="1024" >
    <Grid>

        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Margin="108,81,88,92" Width="329" Height="177">
            <Button Content="Admin"    Command="{Binding IniciarAdminCommand}" HorizontalAlignment="Left" VerticalAlignment="Center" Width="329" Height="54"/>
            <Button Content="App"    Command="{Binding IniciarAppCommand}" HorizontalAlignment="Left" Margin="0,30"  VerticalAlignment="Center" Width="329" Height="52"/>
        </StackPanel>
    </Grid>
</Window>
